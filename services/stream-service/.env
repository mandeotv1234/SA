# Stream Service configuration
PORT=3000

# Kafka
KAFKA_BROKERS=kafka:9092
KAFKA_CLIENT_ID=stream-service
MARKET_DATA_TOPIC=market_data

# Binance subscriptions (comma separated, lower-case symbols)
SYMBOLS=btcusdt,ethusdt

# Kline interval (1m, 5m, etc.)
INTERVAL=1m

# Whether to forward partial (in-progress) klines. "false" = only final candles.
SEND_PARTIAL=false

# Optional
LOG_LEVEL=info
SOCKET_PATH=/socket.io
NODE_ENV=production